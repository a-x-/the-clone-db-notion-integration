{
    "name": "notion-database-cloner",
    "version": "2.0.0",
    "description": "Notion API integration for duplicating databases via webhook",
    "type": "module",
    "scripts": {
        "dev": "vercel dev",
        "test": "vitest --run",
        "test:watch": "vitest --watch",
        "test:ui": "vitest --ui",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "format": "biome format --write .",
        "type-check": "tsc --noEmit",
        "deploy": "vercel --prod"
    },
    "dependencies": {
        "@notionhq/client": "^2.2.15"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.8.3",
        "@types/node": "^20.14.9",
        "@vercel/node": "^3.2.0",
        "@vitest/ui": "^1.6.0",
        "typescript": "^5.5.2",
        "vercel": "^43.3.0",
        "vitest": "^1.6.0",
        "@types/bun": "latest"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "notion",
        "api",
        "webhook",
        "database",
        "clone",
        "typescript"
    ],
    "author": "ultra",
    "license": "MIT",
    "private": true
}
